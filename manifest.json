{
"manifest_version": 2,
    "name": "Julia's Chrome text/replace",
    "description": "Replaces text on webpage",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}